  <Variable
    Name="P250_FROM_DATE"
    Kind="where"
    Type="oracle.jbo.domain.Date"/>
  <Variable
    Name="P250_TO_DATE"
    Kind="where"
    Type="oracle.jbo.domain.Date"/>
  <SQLQuery><![CDATA[select a.DN_CN_NO ,a.DN_CN_DT,a.FIN_TVOUCH_UNIT_CODE ,a.NARRATION , 
a.BILL_NO_FOR_CREDIT,a.BILL_DATE_FOR_CREDIT,c.PARTY_TYPE,a.SUB_CODE,c.name,D.ADDRESS1 AS ADDRESS,c.GST_REG_NO,D.STATE_GST_CODE||'-'||D.STATE  as "State Code", 
b.ITEM_CD,(SELECT ITEM_DESC FROM ITEM_STOCK_ALL WHERE ITEM_CD = B.ITEM_CD) ITEM_DESCRIPTION, 
(SELECT UOM FROM PRODUCTS WHERE PRODUCT_CODE= B.ITEM_CD 
UNION 
SELECT MEASUREMENT_UNIT FROM PART_MASTER WHERE PART_CODE= B.ITEM_CD)ITEM_UOM, 
E.GL_CODE,F.GL_DESC, 
b.HSN_NO,b.QTY ,(b.AMT-b.DISCOUNT_AMT)as Amount,b.DCGST_RATE,b.DCGST_AMT ,b.DSGST_RATE,b.DSGST_AMT,b.DIGST_RATE ,b.DIGST_AMT , 
(b.DCGST_AMT+b.DSGST_AMT+DIGST_AMT+(b.AMT-b.DISCOUNT_AMT))as "Total Amount"       ---  
 from TERMS.dr_cr_note a 
LEFT JOIN dr_cr_detail_item b on  a.DN_CN_NO =  b.DR_CR_NOTE_DN_CN_NO 
LEFT JOIN vendor_master c on a.sub_code = c.VENDOR_CODE 
LEFT JOIN VENDOR_REGD_ADDRESS D ON C.VENDOR_CODE = D.VENDOR_CODE  
 LEFT JOIN INV_SER_BILL_VIEW E ON  B.ITEM_CD= E.ITEM_CD 
LEFT JOIN GEN_LED_V10 F ON E.GL_CODE = F.GL_CD  
 where (a.FIN_TVOUCH_UNIT_CODE = :P250_UNIT_CODE or 'ALL'=:P250_UNIT_CODE) 
and a.DN_CN_DT  between :P250_FROM_DATE and :P250_TO_DATE 
AND AUTH_BY IS NOT NULL]]></SQLQuery>
