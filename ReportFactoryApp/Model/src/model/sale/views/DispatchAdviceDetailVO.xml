  <Variable
    Name="P217_DA_NO"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[select  
    D.DA_NO, 
    (SELECT SAP_NUMBER FROM PRODUCT WHERE CODE = D.PRODUCT_CODE) SAP_NO, 
    D.PRODUCT_CODE, 
    (SELECT DESCRIPTION FROM PRODUCT WHERE CODE = D.PRODUCT_CODE) DESCRIPITION, 
    D.HSN_NO,D.MRP,D.BATCH_NO,D.MFG_DT,D.EXP_DT,D.UOM,D.QTY,D.RATE,--D.GST_CODE  
     (select GST_RATE from gst_rate_master where GST_CODE=D.GST_CODE) GST_RATE, 
         D.PACK_QTY PACK_SIZE, 
         (select distinct CONVER_FACT from item_packing_uom where ITEM_CD=D.PRODUCT_CODE and unit_cd=h.unit_cd)standard_size 
FROM DESPATCH_ADVICE_DETAIL D, 
     DESPATCH_ADVICE_HEADER H 
WHERE H.DA_NO=D.DA_NO 
AND H.DA_DATE BETWEEN :P217_FR_DT AND :P217_TO_DT 
AND D.DA_NO LIKE :P217_DA_NO||'%']]></SQLQuery>
