    Name="P182_TO_DATE"
    Kind="where"
    Type="oracle.jbo.domain.Date"/>
  <SQLQuery><![CDATA[SELECT H.UNIT_CODE, D.PO_HEAD_ACK_NUMBER PO_NO, TO_CHAR(D.CREATION_DATE,'DD/MM/YYYY') PO_DATE,  
H.CUST_PO_NUMBER, H.CUST_PO_DATE, H.Valid_upto, D.Prod_Code, D.QUANTITY, D.PRICE, D.DIS_PER, D.Discount,  
(NVL(QUANTITY,0)* NVL(PRICE,0)-NVL(DISCOUNT,0)) TAXABLE_AMOUNT, D.GST_Code, D.SGST_RATE,  
round(D.SGST,2) SGST, D.CGST_RATE, round(D.CGST,2) CGST, D.IGST_RATE, round(D.IGST,2) IGST, 
(NVL(QUANTITY,0) * NVL(PRICE,0)- NVL(DISCOUNT,0) + NVL(ROUND(D.SGST,2),0) + NVL(ROUND(D.CGST,2),0) + NVL(ROUND(D.IGST,2),0)) ORDERVALUE, P.QTY 
FROM PURCHASE_ORDER_HEADER H   
JOIN PURCHASE_ORDER_DETAIL D 
  ON H.ACK_NUMBER = D.PO_HEAD_ACK_NUMBER 
LEFT JOIN DESPATCH_ADVICE_DETAIL P 
  ON H.ACK_NUMBER = P.PO_ACK_NUMBER 
WHERE ( H.UNIT_CODE = :P182_UNIT_CODE OR 'ALL' = :P182_UNIT_CODE ) 
  AND ( H.ACK_NUMBER = :P182_PO_NO OR 'ALL' = :P182_PO_NO ) 
  AND D.CREATION_DATE BETWEEN :P182_FROM_DATE AND :P182_TO_DATE 
ORDER BY 1,3,2]]></SQLQuery>
