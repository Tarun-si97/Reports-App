  <SQLQuery><![CDATA[SELECT P.PRODN_SLIP_NO, P.PRODN_SLIP_DT, P.LOT_NO as "Process Jobcard", P.LEAF_CD, 
(SELECT DESCRIPTION FROM PRODUCT WHERE CODE = P.LEAF_CD 
UNION SELECT DESCRIPTION FROM PART_MASTER WHERE PART_CODE = P.LEAF_CD ) ITEM_NAME , P.UOM, P.PROCESS_CD, P.START_TIME, 
P.END_TIME, P.PRODN_QTY as "Production For Qty", P.ACCEPT_QTY as "Output qty", P.REJECT_QTY, P.PLAN_QTY, P.FROM_LOC, P.TO_DEST, 
P.PDQA_QTY, P.PRODN_HRS, P.UNIT_CD, P.BATCH_NO, ROUND(((P.ACCEPT_QTY*100)/P.PRODN_QTY),3) Yield, 
(SELECT USER_FNAME FROM TERMS.USER_MASTER WHERE EMP_ID=P.LAST_UPDATED_BY) APPROVED_BY 
FROM prodn_slip_head P 
WHERE P.PRODN_SLIP_DT >=:P32_FR_DT AND P.PRODN_SLIP_DT <=:P32_TO_DT 
AND P.UNIT_CD=:P32_UNIT]]></SQLQuery>
