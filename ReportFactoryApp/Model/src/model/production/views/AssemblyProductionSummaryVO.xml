<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="AssemblyProductionSummaryVO"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.production.views.AssemblyProductionSummaryVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="P142_PROD"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="P142_UNIT"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="P142_FROM"
    Kind="where"
    Type="oracle.jbo.domain.Date"/>
  <Variable
    Name="P142_TO"
    Kind="where"
    Type="oracle.jbo.domain.Date"/>
  <SQLQuery><![CDATA[SELECT UNIT_CD,PRODUCT_CODE, ITEM_DESC, SUM(ACCEPT_QTY) PRODN_QTY,  
SUM(ACCEPT_QTY) ACCEPT_QTY, SUM(REJECT_QTY) REJECT_QTY 
FROM ASSY_PRODN_SLIP A,ITEM_STOCK B 
WHERE PRODUCT_CODE=ITEM_CD 
AND ASS_PRODN_SLIP_DT >= :P142_FROM AND ASS_PRODN_SLIP_DT <= :P142_TO 
AND UNIT_CD LIKE :P142_UNIT||'%' 
AND PRODUCT_CODE LIKE :P142_PROD 
GROUP BY UNIT_CD,PRODUCT_CODE, ITEM_DESC 
ORDER BY 1, 2]]></SQLQuery>
  <ViewAttribute
    Name="UnitCd"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="UNIT_CD"
    Expression="UNIT_CD"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="5"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ProductCode"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="25"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PRODUCT_CODE"
    Expression="PRODUCT_CODE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="25"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ItemDesc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="120"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ITEM_DESC"
    Expression="ITEM_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="120"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ProdnQty"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="PRODN_QTY"
    Expression="PRODN_QTY"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="AcceptQty"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="ACCEPT_QTY"
    Expression="ACCEPT_QTY"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="RejectQty"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="REJECT_QTY"
    Expression="REJECT_QTY"
    SQLType="NUMERIC"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
