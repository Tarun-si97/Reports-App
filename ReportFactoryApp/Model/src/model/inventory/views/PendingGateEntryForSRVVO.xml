    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT gh.UNIT_CD, gh.GATE_ENO,gd.ITEM_CD,gd.CHALLAN_QTY,gd.PO_HEAD_PO_NO, 
gh.GATE_EDT, gh.VEN_CD, gh.PO_VEN_NAME VEN_NAME, gh.CHALLAN_TYPE, 
gh.DOC_TYPE, gh.DOC_NO CHALLAN_NO, gh.DOC_DT CHALLAN_DT,gh.BILL_NO, gh.BILL_DT,ROUND(SYSDATE-gh.GATE_EDT) PEND_DAYS, 
(select DESCRIPTION from part_master where PART_CODE=gd.ITEM_CD  
union  
select DESCRIPTION from product where CODE=gd.ITEM_CD)item_description 
FROM GT_EHEADS gh,GT_EDETAILS gd 
WHERE gh.SRV_HEAD_SRV_NO IS NULL  
and gh.GATE_ENO=gd.GT_EHEAD_GATE_ENO 
AND gh.UNIT_CD=:P46_UNIT 
AND gh.STATUS <>'C' 
--AND gh.STATUS ='G'  
AND DOC_TYPE IN ('PUC','JOB') 
ORDER BY 1,2,3]]></SQLQuery>
