    Type="oracle.jbo.domain.Date"/>
  <SQLQuery><![CDATA[SELECT A.UNIT_CD,A.CHALLAN_NO, A.CHALLAN_DT,TO_CHAR(PGT_DATE,'DD-MM-YYYY') TARGET_DATE,  
DECODE(A.CHALLAN_TYPE,'SR','MRN Rejection','LR','Line Rejection','JS','Against Job Work','AS','PACKING RETURNABLE','AG','RGP','FO','OTHERS') CHALLAN_TYPE,  
A.PARTY_CD, A.PARTY_NAME, A.EXCISE_TYPE, A.E57F3_NO, A.E57F3_DT, A.GATE_EXIT_NO,  
A.GATE_EXIT_DT, DECODE(A.EXIT_TYPE,'R','RETURNABLE','NON RETURNABLE') EXIT_TYPE, DECODE(A.STATUS,'H','HOLD','C','CANCEL','EXITED') 
STATUS,  
A.FOR_PROCES, A.DEL_MODE, A.VEHICLE_NO, A.TRPT_CD, 
B.DOC_NO,B.ITEM_CD, B.DESCRIPT, B.PROC_CD,B.QTY,UOM, B.AMT, B.REMARKS,GT_EHEAD_GATE_ENO,(select GATE_EDT from gt_eheads where GATE_ENO=GT_EHEAD_GATE_ENO) gate_dt,   
CHALLAN_QTY,EMP_CD,(select EMP_FIRST_NAME||' '||EMP_LAST_NAME from emp_master  where EMP_NUMBER=EMP_CD) emp_name 
FROM CCGP_HEAD A,CCGP_DETLS B,GT_EDETAILS C 
WHERE A.CHALLAN_NO=B.CCGP_HEAD_CHALLAN_NO 
AND A.CHALLAN_NO=C.PO_HEAD_PO_NO(+) 
AND B.ITEM_CD=C.ITEM_CD(+) 
AND  A.CHALLAN_DT BETWEEN :P11_FROM_DATE AND :P11_TO_DATE AND A.UNIT_CD=:P11_UNIT 
ORDER BY 1,3,2]]></SQLQuery>
