<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ItemCodeVVO"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="common.viwes.ItemCodeVVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT ITEM_CD||' - '||ITEM_DESC ITEM_DESC, ITEM_CD FROM (          
SELECT 'ALL' ITEM_DESC, '%' ITEM_CD ,1 FROM DUAL          
UNION ALL          
SELECT ITEM_DESC,A.ITEM_CD,2  FROM STOCK_LEDGER A,ITEM_STOCK B          
WHERE A.ITEM_CD=B.ITEM_CD          
UNION          
SELECT ITEM_DESC,B.ITEM_CD,2 FROM MNTH_BAL A,ITEM_STOCK B          
WHERE A.ITEM_STOCK_ITEM_CD=B.ITEM_CD          
ORDER BY 3)]]></SQLQuery>
  <ViewAttribute
    Name="ItemDesc"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="120"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
